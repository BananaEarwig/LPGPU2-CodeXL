#MAKE FILE FOR CLOccupancyAgent

DEPTH = ../..
include $(DEPTH)/Build/Common.mk

TARGETSO = $(BACKEND_OUTPUT_DIR)/lib$(GPU_PROFILER_LIB_PREFIX)CLOccupancyAgent$(TARGET_SUFFIX).so

INCLUDES = \
	-I. \
	-I$(BACKEND_CLPROFILEAGENT_DIR) \
	-I$(BACKEND_CLTRACEAGENT_DIR) \
	-I$(BACKEND_COMMON_DIR) \
	-I$(BACKEND_CLCOMMON_DIR) \
	-I$(COMMON_PROJ_DIR) \
	-I$(ATI_STREAM_SDK_DIR)/include \
	-I$(CAL_DIR)/include \
	-I$(DYNAMICLIBRARYMODULE_DIR) \
	-I$(DEVICEINFO_DIR) \
	-I$(TSINGLETON_DIR) \
	-I$(AMDTMUTEX_DIR) \
	-I$(UTF8CPP_DIR)

ifeq ($(findstring aarch64-linux-gnu-g++,$(CC)), aarch64-linux-gnu-g++)
INCLUDES += \
	-I/usr/aarch64-linux-gnu-g++/include
endif

ifeq ($(findstring arm-linux-gnueabihf-g++,$(CC)), arm-linux-gnueabihf-g++)
INCLUDES += \
	-I/usr/arm-linux-gnueabihf-g++/include
endif

LIBS = \
	$(CLCOMMON_LIBS) \
	$(COMMON_LIBS) \
	$(FRAMEWORK_LIBS) \
	$(STANDARD_LIBS)

LIBPATH = $(COMMON_LIB_PATHS)

SO_OBJS =  \
	./$(OBJ_DIR)/CLIntercept.o \
	./$(OBJ_DIR)/CLOccupancyAgent.o \
	./$(OBJ_DIR)/CLOccupancyFile.o \
	./$(OBJ_DIR)/CLOccupancyInfoManager.o \

include $(DEPTH)/Build/CommonTargets.mk

# END OF MAKE FILE
